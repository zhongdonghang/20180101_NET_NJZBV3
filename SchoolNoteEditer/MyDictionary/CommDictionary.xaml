					<Grid>
						<Rectangle Stroke="White" RadiusX="5" RadiusY="5" x:Name="bg" Fill="#FF1181E9"/>
						<Viewbox Margin="3,3,3,3">
							<Canvas ClipToBounds="True" d:LayoutOverrides="VerticalMargin" Width="100" Height="100">
								<Line X1="0" X2="100" Y1="0" Y2="100" x:Name="l1" Stroke="White" StrokeThickness="5"/>
								<Line X1="100" X2="0" Y1="0" Y2="100" x:Name="l2" Stroke="White" StrokeThickness="5"/>
