								<Setter TargetName="bg" Property="Stroke" Value="#FF1181E9"/>
								<Setter TargetName="l1" Property="Stroke" Value="#FF1181E9"/>
								<Setter TargetName="l2" Property="Stroke" Value="#FF1181E9"/>
